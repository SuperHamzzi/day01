// 예제1) 
		
		int num1, num2 = 0;
		String result = "";
		Scanner sc = new Scanner(System.in);
		num1 = sc.nextInt();
		num2 = sc.nextInt();
		result = (num1>=60)&&(num2>=60)? "합격" : "불합격";
		System.out.println(result);
		
		int num3, num4 = 0;
		num3 =sc.nextInt();
		num4 =sc.nextInt();
		if(num3>num4) {
			System.out.println("큰 수:"+num3+"  작은 수:"+num4);
		}else if(num3==num4) {
			System.out.println("값이 같습니다.");
		}
		else {
			System.out.println("큰 수:"+num4+" 작은 수" + num3);
		}
		
		int num5 = sc.nextInt();
		result = num5==3? "같은수" :"다른 수";
		System.out.println(result);
		String result2 ="";
		String name =sc.next();
		result2 = (name=="홍길동") ? "같은사람" :"다른 사람";
		
		
		
		
		
		
	}

}
